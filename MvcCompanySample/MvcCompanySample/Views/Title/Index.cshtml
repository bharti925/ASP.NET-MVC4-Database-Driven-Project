@model IEnumerable<MvcCompanySample.Models.Title>

@{
    ViewBag.Title = "Enter Title";
}

<h2>Enter Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TitleId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TitleId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TitleId })
        </td>
    </tr>
}

</table>
